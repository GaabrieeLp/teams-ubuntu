{
  "name": "teams-ubuntu",
  "version": "1.1.6",
  "description": "Microsoft teams for ubuntu platforms",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "rm -rf dist && npm run 32-bit && npm run 64-bit",
    "32-bit": "electron-packager . 'Microsoft Teams' --platform=linux --arch=ia32 --out=./dist --app-version=1.1.7 --ignore=\"node_modules/\"",
    "64-bit": "electron-packager . 'Microsoft Teams' --platform=linux --arch=x64 --out=./dist --app-version=1.1.7 --ignore=\"node_modules/\"",
    "distMac": "electron-packager . 'Microsoft Teams' --platform=darwin --arch=x64 --out=./dist --app-version=1.1.7 --ignore=\"(dist/|node_modules/)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/teams-ubuntu.git"
  },
  "keywords": [
    "electron",
    "teams"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/teams-ubuntu/issues"
  },
  "homepage": "https://github.com/leftstick/teams-ubuntu#readme",
  "devDependencies": {
    "electron": "^2.0.5",
    "electron-packager": "^12.1.0"
  }
}
